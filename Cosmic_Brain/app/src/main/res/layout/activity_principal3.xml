<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Principal3">

    <TextView
        android:id="@+id/idLabelSe2"
        android:layout_width="277dp"
        android:layout_height="56dp"
        android:layout_marginTop="140dp"
        android:layout_marginEnd="32dp"
        android:text="Choose the Quiz"
        android:textSize="32sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.843"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/idQuizz4"
        android:layout_width="269dp"
        android:layout_height="65dp"
        android:backgroundTint="#03A9F4"
        android:onClick="irParaQuizz2"
        android:text="QUIZ 2"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idQuizz3"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/idQuizz3"
        android:layout_width="269dp"
        android:layout_height="65dp"
        android:layout_marginTop="88dp"
        android:backgroundTint="#03A9F4"
        android:onClick="irParaQuizz"
        android:text="QUIZ 1"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idLabelSe2" />

</androidx.constraintlayout.widget.ConstraintLayout>